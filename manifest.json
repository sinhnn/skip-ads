{
    "name": "Simple Skip Ads",
    "version": "1.0",
    "description": "Description",
    "manifest_version": 3,
    "author": "SinhNN",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "run_at": "document_end",
            "js": ["js/index.js"]
        }
    ],
    "permissions": ["activeTab", "storage", "tabs", "scripting"],
    "host_permissions": ["*://*.youtube.com/*"]
}